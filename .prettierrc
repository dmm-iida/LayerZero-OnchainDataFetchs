{
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "all",
        "semi": true,
        "bracketSpacing": true,
        "organizeImportsSkipDestructiveCodeActions": true
      }
    }
  ]
}
